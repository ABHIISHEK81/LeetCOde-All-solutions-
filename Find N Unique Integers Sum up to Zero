//  1304. Find N Unique Integers Sum up to Zero  //





class Solution(object):
    def sumZero(self, n):
        """
        :type n: int
        :rtype: List[int]
        """
        r = []
        if n%2 != 0:
            r.append(0)
        x = n//2
        for i in range(1, x+1):
            r.append(i)
            r.append(-i)
        return r        
