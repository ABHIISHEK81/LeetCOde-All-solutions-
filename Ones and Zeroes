//  474. Ones and Zeroes  //










class Solution(object):
    def findMaxForm(self, strs, m, n):
        """
        :type strs: List[str]
        :type m: int
        :type n: int
        :rtype: int
        """
        dp = {(0, 0): 0}  
        for s in strs:
            zeros = s.count('0')
            ones = s.count('1')

            new_dp = dict(dp)

            for (z, o), size in dp.items():
                new_z, new_o = z + zeros, o + ones

                if new_z <= m and new_o <= n:
                    new_dp[(new_z, new_o)] = max(new_dp.get((new_z, new_o), 0), size + 1)

            dp = new_dp 

        return max(dp.values())
