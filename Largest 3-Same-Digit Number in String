//    2264. Largest 3-Same-Digit Number in String   // 





class Solution(object):
    def largestGoodInteger(self, num):
        """
        :type num: str
        :rtype: str
        """
        d = ['999','888', '777', '666', '555', '444', '333', '222', '111', '000']
        for val in d:
            if val in num:
                return val
        return ''    
