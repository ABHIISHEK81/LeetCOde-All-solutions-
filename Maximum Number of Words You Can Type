// 1935. Maximum Number of Words You Can Type //





class Solution(object):
    def canBeTypedWords(self, text, brokenLetters):
        """
        :type text: str
        :type brokenLetters: str
        :rtype: int
        """
        count=0
        arr=text.split(" ")
        for i in arr:
            for j in brokenLetters:
                if j in i:
                    count+=1
                    break
        return len(arr)-count
        
