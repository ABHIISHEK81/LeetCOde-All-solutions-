// 3314. Construct the Minimum Bitwise Array I //



class Solution(object):
    def minBitwiseArray(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        ans  = []
        
        for x in  nums:
            idx = -1
            for j in range(1, x):
                if (j | (j + 1)) == x:
                    idx = j
                    break
            ans.append(idx)

        return ans           
