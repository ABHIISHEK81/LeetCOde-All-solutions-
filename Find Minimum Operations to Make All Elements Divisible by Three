//  3190. Find Minimum Operations to Make All Elements Divisible by Three  //










class Solution(object):
    def minimumOperations(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        cnt1 = cnt2 = 0
        for x in nums:
           r = x % 3
           if r == 1: cnt1 += 1
           elif r == 2: cnt2 += 1
        return cnt1 + cnt2
