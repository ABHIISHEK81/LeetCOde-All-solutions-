/ Check If All 1's Are at Least Length K Places Away  /










class Solution(object):
    def kLengthApart(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: bool
        """
        distance = 0
        found = False

        for num in nums:
            if num == 1:

                if distance <= k and found:
                    return False

                found = True

                distance = 0

            distance += 1

        return True            
