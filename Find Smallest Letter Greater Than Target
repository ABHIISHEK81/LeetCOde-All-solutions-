//  744. Find Smallest Letter Greater Than Target //



class Solution(object):
    def nextGreatestLetter(self, letters, target):
        """
        :type letters: List[str]
        :type target: str
        :rtype: str
        """
        left = 0
        right = len(letters)-1
        while left<right:
            mid = (left+right)//2
            if letters[mid] <= target:
                left = mid+1
            else:
                right = mid
        if letters[left] > target:
            return letters[left]
        return letters[0]
